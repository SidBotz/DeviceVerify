{
  "version": 2,
  "builds": [
    { "src": "redirect.js", "use": "@vercel/node" },
    { "src": "index.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/(?<userid>[^/]+)/(?<linktype>[^/]+)/(?<redirectlink>[^/]+)", "dest": "/redirect.js" },
    { "src": "/", "dest": "index.html" }
  ]
}
